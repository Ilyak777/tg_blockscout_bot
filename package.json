    "exclude": [
      "node_modules/**/*",
      "test/**/*"
    ],
    "check-coverage": true,
    "lines": 75
  },
  "pre-commit": [
    "pre-commit"
  ],
  "lint-staged": {
    "*.js": [
      "prettier --write --config .prettierrc.json"
    ]
  }
