    "json-rpc-2.0": "^1.7.0",
    "lodash": "4.17.21",
    "moment-timezone": "0.5.43",
    "morgan": "1.10.0",
    "pg": "^8.12.0",
    "pm2-runtime": "^5.4.0",
    "prom-client": "^15.1.3",
    "response-time": "^2.3.2",
    "sanitize-html": "2.10.0",
    "sinon": "^17.0.1",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
