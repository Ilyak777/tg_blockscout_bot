{
  "name": "api",
  "version": "2.0.3",
  "description": "",
  "main": "index.js",
  "scripts": {
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migrate": "npm run typeorm -- migration:run -d src/data/index.ts",
    "test": "mocha",
    "test:specific": "node --expose-gc ./node_modules/.bin/mocha",
    "coverage": "nyc --reporter=lcov --reporter=text  npm run test",
    "coverage:specific": "nyc mocha ",
    "build": "rimraf ./dist && tsc",
    "start": "nodemon",
    "stop": "pm2 stop 'etx-summary' && pm2 delete 'etx-summary'",
    "start:dev": "nodemon",
    "pre-commit": "lint-staged",
    "lint:fix": "prettier --write --ignore-path .eslintignore . ;  eslint --ignore-path .eslintignore src/ --fix; tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blockscout/eth-tx-summary-api"
  },
  "keywords": [],
  "homepage": "https://github.com/blockscout/eth-tx-summary-api",
  "dependencies": {
